{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-console": "off",
        "no-use-before-define": "off",
        "consistent-return": "off",
        "global-require": "off",
        "no-plusplus": "off",
        "arrow-body-style": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
    }
}